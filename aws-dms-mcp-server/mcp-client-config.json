{
  "mcpServers": {
    "awslabs.aws-dms-mcp-server": {
      "command": "python",
      "args": [
        "-m",
        "awslabs.aws_dms_mcp_server.server"
      ],
      "cwd": "/path/to/aws-dms-mcp-server",
      "env": {
        "AWS_REGION": "us-east-1",
        "DMS_LOG_LEVEL": "INFO",
        "DMS_READ_ONLY_MODE": "false"
      }
    }
  }
}
